program 11

SQL> set serveroutput on ;
SQL> create or replace procedure welcome_msg as
  2  begin
  3  dbms_output.put_line('welcome to pl/sql procedures!');
  4  end;
  5  /

SQL> exec welcome_msg;
_welcome to pl/sql procedures!_



SET SERVEROUTPUT ON;
DECLARE
  a NUMBER:=10;
  b NUMBER:=5;
BEGIN
  IF a<b THEN
    DBMS_OUTPUT.PUT_LINE('Min='||a);
  ELSE
    DBMS_OUTPUT.PUT_LINE('Min='||b);
  END IF;
END;
/
